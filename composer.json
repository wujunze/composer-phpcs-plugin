{
    "description": "Composer plugin for code style checking before commit using php-cs-fixer",
    "name": "wujunze/composer-phpcs-plugin",
    "license": "MIT",
    "type": "composer-plugin",
    "require": {
        "composer-plugin-api": "^1.1",
        "friendsofphp/php-cs-fixer": "^2.15"
    },
    "autoload": {
        "psr-4": {
            "leap\\composer\\": "src/"
        }
    },
    "extra": {
        "class": "leap\\composer\\PhpcsPlugin"
    }
}
